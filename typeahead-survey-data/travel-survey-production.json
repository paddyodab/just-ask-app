{
  "title": "World Travel Experience Survey",
  "description": "Share your international travel experiences and help us understand global travel patterns",
  "logo": "/assets/travel-logo.png",
  "logoPosition": "top",
  "logoWidth": "150px",
  "pages": [
    {
      "name": "personal_info",
      "title": "About You",
      "description": "Let's start with some basic information",
      "elements": [
        {
          "type": "dropdown",
          "name": "country_of_origin",
          "title": "What is your country of origin?",
          "isRequired": true,
          "placeholder": "Start typing your country...",
          "enableTypeahead": true,
          "typeaheadMinLength": 2,
          "typeaheadDebounceMs": 300,
          "choicesByUrl": {
            "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/countries",
            "valueName": "key",
            "titleName": "text"
          }
        },
        {
          "type": "dropdown",
          "name": "city_of_origin",
          "title": "What city are you originally from?",
          "isRequired": true,
          "placeholder": "Start typing your city...",
          "enableTypeahead": true,
          "typeaheadMinLength": 2,
          "typeaheadDebounceMs": 300,
          "choicesByUrl": {
            "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/cities?country={country_of_origin}",
            "valueName": "key",
            "titleName": "text"
          },
          "visibleIf": "{country_of_origin} notempty"
        },
        {
          "type": "dropdown",
          "name": "age_range",
          "title": "What is your age range?",
          "isRequired": true,
          "choices": [
            { "value": "18-24", "text": "18-24 years" },
            { "value": "25-34", "text": "25-34 years" },
            { "value": "35-44", "text": "35-44 years" },
            { "value": "45-54", "text": "45-54 years" },
            { "value": "55-64", "text": "55-64 years" },
            { "value": "65+", "text": "65 years and above" }
          ]
        },
        {
          "type": "dropdown",
          "name": "travel_frequency",
          "title": "How often do you travel internationally?",
          "isRequired": true,
          "choices": [
            { "value": "never", "text": "This will be my first time" },
            { "value": "rarely", "text": "Once every few years" },
            { "value": "annually", "text": "Once or twice a year" },
            { "value": "frequently", "text": "3-5 times a year" },
            { "value": "very_frequently", "text": "More than 5 times a year" }
          ]
        }
      ]
    },
    {
      "name": "countries_visited",
      "title": "Countries You've Visited",
      "description": "Tell us about the countries you've explored",
      "elements": [
        {
          "type": "panel",
          "name": "travel_intro",
          "elements": [
            {
              "type": "html",
              "name": "intro_text",
              "html": "<p style='font-size: 14px; color: #666;'>Please add all the countries you've visited. You can add multiple entries.</p>"
            }
          ]
        },
        {
          "type": "paneldynamic",
          "name": "visited_countries",
          "title": "Countries I've Visited",
          "templateElements": [
            {
              "type": "dropdown",
              "name": "country",
              "title": "Country",
              "isRequired": true,
              "placeholder": "Type to search for a country...",
              "enableTypeahead": true,
              "typeaheadMinLength": 2,
              "typeaheadDebounceMs": 300,
              "choicesByUrl": {
                "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/countries",
                "valueName": "key",
                "titleName": "text"
              }
            },
            {
              "type": "dropdown",
              "name": "favorite_city",
              "title": "Your favorite city in this country",
              "placeholder": "Type to search for a city...",
              "enableTypeahead": true,
              "typeaheadMinLength": 2,
              "typeaheadDebounceMs": 300,
              "choicesByUrl": {
                "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/cities?country={panel.country}",
                "valueName": "key",
                "titleName": "text"
              },
              "visibleIf": "{panel.country} notempty"
            },
            {
              "type": "rating",
              "name": "country_rating",
              "title": "How would you rate your experience?",
              "isRequired": true,
              "rateMin": 1,
              "rateMax": 5,
              "rateStep": 1,
              "minRateDescription": "Poor",
              "maxRateDescription": "Excellent"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 50,
          "panelAddText": "Add Another Country",
          "panelRemoveText": "Remove Country"
        }
      ]
    },
    {
      "name": "travel_preferences",
      "title": "Travel Preferences",
      "description": "Help us understand what you look for when traveling",
      "elements": [
        {
          "type": "checkbox",
          "name": "travel_interests",
          "title": "What are your main interests when traveling? (Select all that apply)",
          "isRequired": true,
          "choices": [
            { "value": "culture", "text": "Culture & History" },
            { "value": "nature", "text": "Nature & Wildlife" },
            { "value": "food", "text": "Food & Cuisine" },
            { "value": "adventure", "text": "Adventure & Sports" },
            { "value": "relaxation", "text": "Relaxation & Wellness" },
            { "value": "business", "text": "Business & Networking" },
            { "value": "shopping", "text": "Shopping" },
            { "value": "nightlife", "text": "Nightlife & Entertainment" }
          ]
        },
        {
          "type": "dropdown",
          "name": "preferred_airline",
          "title": "What is your preferred airline?",
          "placeholder": "Type to search for an airline...",
          "enableTypeahead": true,
          "typeaheadMinLength": 2,
          "typeaheadDebounceMs": 300,
          "choicesByUrl": {
            "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/airlines",
            "valueName": "key",
            "titleName": "text"
          }
        },
        {
          "type": "dropdown",
          "name": "home_airport",
          "title": "What is your home airport?",
          "placeholder": "Type airport name or code...",
          "enableTypeahead": true,
          "typeaheadMinLength": 2,
          "typeaheadDebounceMs": 300,
          "choicesByUrl": {
            "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/airports",
            "valueName": "key",
            "titleName": "text"
          }
        },
        {
          "type": "radiogroup",
          "name": "accommodation_preference",
          "title": "Preferred accommodation type",
          "isRequired": true,
          "choices": [
            { "value": "hotel", "text": "Hotels" },
            { "value": "airbnb", "text": "Airbnb/Vacation Rentals" },
            { "value": "hostel", "text": "Hostels" },
            { "value": "resort", "text": "Resorts" },
            { "value": "friends", "text": "Friends/Family" }
          ]
        }
      ]
    },
    {
      "name": "future_plans",
      "title": "Future Travel Plans",
      "description": "Where would you like to go next?",
      "elements": [
        {
          "type": "paneldynamic",
          "name": "bucket_list",
          "title": "My Travel Bucket List",
          "templateElements": [
            {
              "type": "dropdown",
              "name": "dream_country",
              "title": "Country you want to visit",
              "isRequired": true,
              "placeholder": "Type to search...",
              "enableTypeahead": true,
              "typeaheadMinLength": 2,
              "typeaheadDebounceMs": 300,
              "choicesByUrl": {
                "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/countries",
                "valueName": "key",
                "titleName": "text"
              }
            },
            {
              "type": "dropdown",
              "name": "dream_city",
              "title": "Specific city (optional)",
              "placeholder": "Type to search...",
              "enableTypeahead": true,
              "typeaheadMinLength": 2,
              "typeaheadDebounceMs": 300,
              "choicesByUrl": {
                "url": "/30f8f53cf8034393b00665f664a60ddb/typeahead-demo/lookups/cities?country={panel.dream_country}",
                "valueName": "key",
                "titleName": "text"
              },
              "visibleIf": "{panel.dream_country} notempty"
            },
            {
              "type": "text",
              "name": "visit_reason",
              "title": "Why do you want to visit?",
              "placeholder": "e.g., See the Northern Lights, Visit Machu Picchu..."
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 10,
          "panelAddText": "Add Another Destination",
          "panelRemoveText": "Remove Destination"
        },
        {
          "type": "radiogroup",
          "name": "next_trip_timeline",
          "title": "When are you planning your next international trip?",
          "choices": [
            { "value": "1-3months", "text": "Within the next 3 months" },
            { "value": "3-6months", "text": "3-6 months" },
            { "value": "6-12months", "text": "6-12 months" },
            { "value": "1-2years", "text": "1-2 years" },
            { "value": "unsure", "text": "Not sure yet" }
          ]
        }
      ]
    },
    {
      "name": "feedback",
      "title": "Additional Comments",
      "elements": [
        {
          "type": "comment",
          "name": "travel_story",
          "title": "Share your most memorable travel experience (optional)",
          "placeholder": "Tell us about a special moment from your travels...",
          "rows": 5
        },
        {
          "type": "boolean",
          "name": "newsletter_signup",
          "title": "Would you like to receive travel tips and destination guides?",
          "defaultValue": false
        }
      ]
    }
  ],
  "showProgressBar": "top",
  "progressBarType": "pages",
  "completeText": "Submit Survey",
  "completedHtml": "<h3>Thank you for sharing your travel experiences!</h3><p>Your responses will help us better understand global travel patterns and preferences.</p>",
  "showQuestionNumbers": "off",
  "questionTitleLocation": "top",
  "requiredText": "*",
  "questionsOnPageMode": "standard"
}